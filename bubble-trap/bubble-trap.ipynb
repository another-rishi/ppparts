   "source": [
    "import numpy as np\n",
    "from pint import UnitRegistry\n",
    "\n",
    "units = UnitRegistry()\n",
    "\n",
    "h = 100 * units.mm\n",
    "r = 7.5 * units.mm\n",
    "\n",
    "V = np.pi * r**2 * h\n",
    "rho_h20 = (1 * units.ml) / (1 * units.cm**3)\n",
    "\n",
    "v = V * rho_h20\n",
    "v.to_compact()\n"
   ]
  }
